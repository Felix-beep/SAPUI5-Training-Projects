<mvc:View controllerName="sap.ui.calculator.app.controller.History"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc">
    <Page title="{i18n>homePageTitle}" showHeader="true" showNavButton="true">
        <customHeader>
            <core:Fragment fragmentName="sap.ui.calculator.app.view.Fragments.Navbar" type="XML"/>
        </customHeader>

        <content>
            <core:Fragment fragmentName="sap.ui.calculator.app.view.Fragments.SessionHistory" type="XML"/>
        </content>
    </Page>
</mvc:View>