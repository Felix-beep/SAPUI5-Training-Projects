{
    "_version": "1.58.0",
    "sap.app": {
        "id": "ui5.calculator",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}"
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "resources": {
            "js": [
                {
                    "uri": "<uri>"
                }
            ],
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "dependencies": {
            "minUI5Version": "1.60",
            "libs": {
                "sap.m": {}
            }
            
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "sap.ui.calculator.app.i18n.i18n",
                    "supportedLocales": ["en"],
                    "fallbackLocale": "en"
                },
                "uri": "<uriRelativeToManifest>"
            }
        },
        "rootView": {
            "viewName": "sap.ui.calculator.app.View.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "handleValidation": false,
        "config": {
          
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "sap.ui.calculator.app.View",
                "controlId": "app",
                "controlAggregation": "pages",
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "calculator",
                    "target": "calculator"
                }
            ],
            "targets": {
                "calculator": {
                    "viewId": "calculator",
                    "viewName": "Calculator"
                }
            }
        }
    }
}