{
    "_version": "1.58.0",
    "sap.app": {
        "id": "sap.ui.calculator.app",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "calculationRemote": {
                "uri": "/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "Service/metadata.xml"
                }
            }
        }
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "resources": {
            "js": [
                {
                    "uri": "<uri>"
                }
            ],
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "dependencies": {
            "minUI5Version": "1.60",
            "libs": {
                "sap.m": {}
            }
            
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "sap.ui.calculator.app.i18n.i18n",
                    "supportedLocales": ["en"],
                    "fallbackLocale": "en"
                },
                "uri": "<uriRelativeToManifest>"
            },
            "CalcModel": {
                "dataSource": "calculationRemote",
                "type": "sap.ui.model.odata.v2.ODataModel"
            }
        },
        "rootView": {
            "viewName": "sap.ui.calculator.app.View.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "handleValidation": false,
        "config": {
          
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "sap.ui.calculator.app.View",
                "controlId": "app",
                "controlAggregation": "pages",
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "mainView",
                    "target": "mainView"
                },
                {
                    "pattern": "Login",
                    "name": "loginscreen",
                    "target": "loginscreen"
                },
                {
                    "pattern": "History",
                    "name": "fullhistory",
                    "target": "fullhistory"
                }
            ],
            "targets": {
                "mainView": {
                    "viewId": "mainView",
                    "viewName": "MainView"
                },
                "loginscreen": {
                    "viewId": "loginscreen",
                    "viewName": "Login"
                },
                "fullhistory": {
                    "viewId": "fullhistory",
                    "viewName": "History"
                }
            }
        }
    }
}